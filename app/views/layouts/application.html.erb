<!DOCTYPE html>
<html>
<head>
  <title>Mojoku</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>


  <%= render 'shared/facebook_init' %>


</head>
<body>

	<div id="logo">

		<%logo = image_tag("logo.jpg", :alt => "Mojoku!") %>
		<%= link_to logo, root_url %>

	</div>
      
	<div id='nav'>
	
	      
		<%= render 'shared/account_nav' %>

	
	</div>
	<div >
      		<%= render 'shared/site_nav' %>
		<div id="notice"><%=notice%></div>
	</div>
		<div id="content">

      
		<%= yield %>
      
		
      
	</div>


</body>
</html>
